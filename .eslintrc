{
    "extends": [
      "react-app",
      "airbnb",
      "plugin:jsx-a11y/recommended",
      "prettier"
    ],
    "plugins": [
      "jsx-a11y",
      "prettier"
    ],
    "rules": {
      "semi": ["error", "always"],
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      // "prettier/prettier": [
      //   "error", {
      //     "semi": false
      //   }
      // ]
      "react/react-in-jsx-scope": "off",
      "react/jsx-no-useless-fragment": "off"


    }
  }